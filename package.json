    "xhr": "^2.4.0"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
